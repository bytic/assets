{
  "name": "bytic/assets",
  "license": "",
  "description": "Assets builder for bytic. Build on symfony encore",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "require": {
    "bytic/collections": "^1.0",
    "bytic/utility": "^1.0",
    "symfony/webpack-encore-bundle": "^1.7|^2.0"
  },
  "require-dev": {
    "bytic/view": "^1.0|^2.0",
    "bytic/config": "^1.0|^2.0",
    "bytic/container": "^1.0|^2.0",
    "bytic/cache": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0.19"
  },
  "autoload": {
    "psr-4": {
      "ByTIC\\Assets\\": "src/"
    },
    "files": [
      "src/functions/assets.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Assets\\Tests\\": "tests/src",
      "ByTIC\\Assets\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "bytic/phpqatools": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
