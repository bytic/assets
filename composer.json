{
  "name": "bytic/assets",
  "license": "",
  "description": "Assets builder for bytic. Build on symfony encore",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "require": {
    "bytic/collections": "^1.0",
    "bytic/utility": "^1.0",
    "symfony/webpack-encore-bundle": "^1.7"
  },
  "require-dev": {
    "bytic/view": "^0.9|^1.0",
    "bytic/config": "^0.9|^1.0",
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/cache": "^0.9|^1.0",
    "phpunit/phpunit": "^7.2|^8.0|^9.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ByTIC\\Assets\\": "src/"
    },
    "files": [
      "src/functions/assets.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Assets\\Tests\\": "tests/src",
      "ByTIC\\Assets\\Tests\\Fixtures\\": "tests/fixtures"
    }
  }
}
